{
    "name": "rock-symphony/propel-orm-plugin",
    "type": "symfony1-plugin",
    "description": "Rock Symphony plugin for Propel.",
    "license": "MIT",
    "keywords": ["symfony", "propel", "orm", "active record", "mapping", "database", "persistence"],
    "homepage": "http://www.propelorm.org/",
    "require": {
        "php": "^7.4||^8.0",
        "composer/installers": "*",
        "rock-symphony/propel-orm": "^2.0",
        "rock-symphony/rock-symphony": "^12.0.0",
        "psr/log": "~1.0 || ~2.0 || ~3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
